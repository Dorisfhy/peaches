{
    "rules": [
        {
            "charset": "'iso-8859-15'"
        },
        {
            "import": "'custom.css'"
        },
        {
            "import": "\"2.css\" screen, projection"
        },
        {
            "media": "print",
            "rules": [
                {
                    "selectors": [
                        "html"
                    ],
                    "declarations": [
                        {
                            "property": "background",
                            "value": "#fff" 
                        },
                        {
                            "property": "color",
                            "value": "#000" 
                        }
                    ]
                },
                {
                    "selectors": [
                        "p"
                    ],
                    "declarations": [
                        {
                            "property": "background",
                            "value": "#fff" 
                        },
                        {
                            "property": "color",
                            "value": "#000" 
                        },
                        {
                            "property": "padding",
                            "value": "10px" 
                        }
                    ]
                },
                {
                    "selectors": [
                        "body"
                    ],
                    "declarations": [
                        {
                            "property": "padding",
                            "value": "1in"  
                        },
                        {
                            "property": "border",
                            "value": "0.5pt solid #666" 
                        }
                    ]
                }
            ]
        },
        {
            "selectors": ["html"],
            "declarations": [
                {
                    "property": "background-color",
                    "value": "#fff"
                },
                {
                    "property": "padding",
                    "value": "0"
                },
                {
                    "property": "color",
                    "value": "#000"
                }
            ]
        },
        {
            "selectors": ["body"],
            "declarations": [
                {
                    "property": "background-color",
                    "value": "#fff"
                },
                {
                    "property": "padding",
                    "value": "10px"
                }
            ]
        },
        {
            "selectors": [".test1"],
            "declarations": [
                {
                    "property": "padding",
                    "value": "20px"
                },
                {
                    "property": "margin",
                    "value": "10px"
                },
                {
                    "property": "border",
                    "value": "1px !important",
                    "important": true
                }
            ]
        }
        ,
        {
            "selectors": [".test2"],
            "declarations": [
                {
                    "property": "padding",
                    "value": "10px"
                },
                {
                    "property": "margin",
                    "value": "0"
                },
                {
                    "property": "border",
                    "value": "1px !important",
                    "important": true
                }
            ]
        }
    ]
}

