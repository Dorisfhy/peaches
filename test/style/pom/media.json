{
    "rules": [
        {
            "media": "screen, projection",
            "rules": [
                {
                    "selectors": [
                        "html"
                    ],
                    "declarations": [
                        {
                            "property": "abackground",
                            "value": "#fffef0"
                        },
                        {
                            "property": "color",
                            "value": "#300"
                        }
                    ]
                },
                {
                    "selectors": [
                        "body"
                    ],
                    "declarations": [
                        {
                            "property": "max-width",
                            "value": "35em"
                        },
                        {
                            "property": "margin",
                            "value": "0 auto"
                        }
                    ]
                }
            ]
        },
        {
            "media": "print",
            "rules": [
                {
                    "selectors": [
                        "html"
                    ],
                    "declarations": [
                        {
                            "property": "abackground",
                            "value": "#fff"
                        },
                        {
                            "property": "color",
                            "value": "#000"
                        }
                    ]
                },
                {
                    "selectors": [
                        ".test"
                    ],
                    "declarations": [
                        {
                            "property": "abackground",
                            "value": "#fff"
                        },
                        {
                            "property": "color",
                            "value": "#fff"
                        },
                        {
                            "property": "padding",
                            "value": "10px"
                        }
                    ]
                },
                {
                    "selectors": [
                        "body"
                    ],
                    "declarations": [
                        {
                            "property": "padding",
                            "value": "1in"
                        },
                        {
                            "property": "border",
                            "value": "0.5pt solid #666"
                        }
                    ]
                }
            ]
        }
    ]
}